<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.iwgang.familiarrecyclerviewdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".LinearLayoutActivity" />
        <activity android:name=".GridLayoutActivity" />
        <activity android:name=".StaggeredGridActivity" />
        <activity android:name=".ImitateListViewDemoActivity" />
        <activity android:name=".ImitateGridViewDemoActivity" />
        <activity android:name=".ImitateStaggeredGridViewDemoActivity" />
        <activity android:name=".ImitateNewListViewDemoActivity" />
    </application>

</manifest>
